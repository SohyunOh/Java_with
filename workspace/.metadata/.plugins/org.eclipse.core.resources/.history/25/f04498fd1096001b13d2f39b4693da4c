
public class For {

	public static void main(String[] args) {
	
		
		// Quiz_01
        // i의 값을 적으시오

        System.out.print("1번 : ");
        for(int i = 0; i<=5; i++) {
            System.out.print(i +" "); // 정답 : 0,1,2,3,4,5
        }
        
        System.out.println("");

        // Quiz_02
        // 2 ~ 9 구구단 출력
        for(int i = 1; i <= 9; i++) {
        	System.out.println();
            for(int j = 2; j <= 9 ; j++ ) {
            	System.out.print( j + "X" + i + "=" + i*j );
            	
            }
        }
        
	}

}
